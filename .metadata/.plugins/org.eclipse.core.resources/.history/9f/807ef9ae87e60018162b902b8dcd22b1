package ss.week1;

public class ThreeWayLamp {
	
	public static final int OFF = 0;
	
	public static final int LOW = 1;
	
	public static final int MED = 2;
	
	public static final int HIGH = 3;
	
	private int light;
	
	public ThreeWayLamp () {
		light = 0;
	}
	

public int light () {
	return light;
	}

public void main change () {
	light = (light + 1) % 4;
	}

}
